{
	"name": "mysecondextention",
	"displayName": "Cat",
	"description": "Some practice in typescript and extention",
	"version": "0.0.2",
	"engines": {
		"vscode": "^0.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:catCoding.start"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mysecondextention.cat",
				"title": "My2Ext"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "^9.4.6",
    	"typescript": "^2.8.3"
	}
}
